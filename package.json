{
  "name": "trendseeker",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "start": "nuxi preview",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lintfix": "prettier --write --list-different . && npm run lint:js -- --fix && npm run lint:style -- --fix",
    "prepare": "husky install",
    "test": "playwright test --max-failures=2",
    "test:ui": "playwright test --ui"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache",
    "*.{css,scss,sass,html,vue}": "stylelint",
    "*.**": "prettier --check --ignore-unknown"
  },
  "dependencies": {
    "@nuxt/image": "0.7.1",
    "core-js": "^3.33.1",
    "nuxt": "^2.17.2",
    "vue": "^2.7.15",
    "vue-server-renderer": "^2.7.15",
    "vue-template-compiler": "^2.7.15"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@cliqz/adblocker-playwright": "^1.26.8",
    "@nuxt/bridge": "npm:@nuxt/bridge-edge@^3.0.0-rc.4-28365925.4df0d6f",
    "@nuxt/types": "^2.17.2",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/google-fonts": "^3.1.3",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@playwright/test": "1.39.0",
    "@types/leaflet": "^1.9.8",
    "@types/node": "^20.8.2",
    "@types/uuid": "^9.0.6",
    "@vue/runtime-dom": "^3.3.11",
    "cross-fetch": "^4.0.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.5.1",
    "fibers": "^5.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.0.4",
    "node-sass": "^8.0.0",
    "nuxi": "^3.10.0",
    "nuxt-compress": "^5.0.0",
    "playwright": "1.39.0",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "sass-loader": "^10.4.1",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^28.0.0"
  }
}